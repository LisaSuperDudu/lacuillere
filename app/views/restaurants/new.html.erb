<h1 class="text-center">Rentrer un nouveau restau</h1>
<div class="container">
  <div class="row">
    <div class="col-xs-12 col-sm-6 col-sm-offset-3">
    <%= simple_form_for @restaurant do |f| %>
    <%= f.input :name %>
      <%= f.input :category %>
      <%= f.input :address %>
      <%= f.button :submit , class: "btn btn-primary" %>
    <% end %>
    </div>
  </div>
</div>




