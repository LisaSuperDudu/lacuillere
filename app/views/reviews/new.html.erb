<h1 class="text-center">Rentrer une nouvelle review pour <%= @restaurant.name%></h1>
<div class="container">
  <div class="row">
    <div class="col-xs-12 col-sm-6 col-sm-offset-3">
    <%= simple_form_for [@restaurant, @review] do |f| %>
      <%= f.input :content %>
      <%= f.input :rating %>
      <%= f.button :submit , class: "btn btn-primary" %>
    <% end %>
    </div>
  </div>
</div>




